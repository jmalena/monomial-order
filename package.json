{
  "name": "monomial-order",
  "version": "0.2.0",
  "description": "Package for finding monomial orders, with optional constraint support.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc && tsc-alias",
    "docs": "typedoc",
    "format": "prettier 'src/**/*.{js,json,ts}' --write",
    "lint": "eslint src",
    "prepare": "npm run build && husky install",
    "test": "jest --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jmalena/monomial-order.git"
  },
  "keywords": [
    "math",
    "ranking",
    "ordering",
    "monomial"
  ],
  "author": "Jonáš Malena <jonas.malena@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jmalena/monomial-order/issues"
  },
  "devDependencies": {
    "@eslint/js": "^9.20.0",
    "@fast-check/jest": "^2.0.3",
    "@types/jest": "^29.5.14",
    "eslint": "^9.20.1",
    "eslint-plugin-tsdoc": "^0.4.0",
    "eslint-plugin-tsdoc-require": "^0.0.3",
    "fast-check": "^3.23.2",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "prettier": "3.5.1",
    "ts-jest": "^29.2.5",
    "tsc-alias": "^1.8.10",
    "typedoc": "^0.27.7",
    "typedoc-github-wiki-theme": "^2.1.0",
    "typedoc-plugin-markdown": "^4.4.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.1"
  }
}
